// orint
#include <stdio.h>

int main() {
    int i;
    int j;

    printf("Numbers from 0-10:\n");
    for(i = 0; i <= 10; i++) {
        printf("%d ", i);
    }

    printf("\n");

    printf("Numbers from 10-0:\n");
    for(j = 10; j >= 0; j--) {
        printf("%d ", j);
    }

    printf("\n");
    return 0;
}
